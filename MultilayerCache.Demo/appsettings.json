{
  "Logging": {
    "MinimumLevel": "Debug",
    "OutputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
  },
  "Cache": {
    "MemoryCacheCleanupIntervalSeconds": 30,
    "DefaultTtlMinutes": 5,
    "Redis": {
      "ConnectionString": "localhost,allowAdmin=true"
    },
    "TotalItems": 10000,
    "WaitForExpirySeconds": 40
  }
}
